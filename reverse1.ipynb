   "source": [
    "def stringreverse(str1):\n",
    "    str2 = ' '\n",
    "    index = len(str1)\n",
    "    while index > 0:\n",
    "        str2 += str1[index -1]\n",
    "        index = index - 1\n",
    "    return str2\n",
    "print(\"the reverse string obtained is :\" , stringreverse('1234abcd'))\n",
    "    "
   ]
